{
    "manifest_version": 3,
    "name": "InstaSave",
    "version": "1.0",
    "description": "Add the options to download posts, stories, and reels from Instagram.",
    "author": "Percy Pham",
    "content_scripts": [
      {
        "js": ["scripts/content.js"],
        "matches": [
          "*://www.instagram.com/*"
        ]
      }
    ],
    "icons": {
      "16": "images/icon16.png",
      "48": "images/icon48.png",
      "128": "images/icon128.png"
    }
  }